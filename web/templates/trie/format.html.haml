- form_for @conn, trie_path(@conn, :format), [as: :trie_form], fn f ->
  %p
    = label f, :trie, "Words for trie, one word per line"
    %br
    = textarea f, :trie, rows: 10, cols: 80, value: @trie_words
  %p
    = submit "render"

%h1 Graphviz Rendering
#graphviz_render

%h1 Elixir Struct
%pre#elixir_struct= @trie_struct

%h1 Graphviz Data
%pre#graphviz_file= @graphviz
